
QTabWidget::pane
{
    border-top: 40px solid #ffffff;
    position: absolute;
    top: -40px;

    color: black;

}

QTabBar::tab
{
    font-size: 14px;
    border-top-right-radius: 10px;
    border-top-left-radius: 10px;
    border-bottom-right-radius: 0px;
    border-bottom-left-radius: 0px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #d3e3f2, stop: 1 #8dbdeb);
    color: black;
    padding: 5px, 5px, 0px, 0px;
    margin: 1px;
}

QTabBar::tab:hover
{
    background: #c1dcf5;
    color: black;
}

QTabBar::tab:selected
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #d7dce0, stop: 1 #9fa2a6);
    color: black;
}

QTabBar::scroller
{
    width: 60px;
    padding: 5px;
}

QTabBar QToolButton::right-arrow
{
    image: url(":/img/arrow-right.png");
    background: #ffffff;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
}

QTabBar QToolButton::left-arrow
{
    image:url(":/img/arrow-left.png");
    background: #ffffff;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
}

QTabBar QToolButton::right-arrow:hover
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #d7dce0, stop: 1 #9fa2a6);
}

QTabBar QToolButton::left-arrow:hover
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #d7dce0, stop: 1 #9fa2a6);
}

QWidget#search_widget QPushButton
{
    font-size: 14px;
    color: black;
    font-weight: normal;
    border: 1px solid black;
    border-radius: 5px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #e1e6eb, stop: 1 #919599);
    padding: 3px, 3px, 3px, 3px;
}

QWidget#search_widget QPushButton::disabled
{
    background-color: #5c5c5c
}

QWidget#search_widget QPushButton::hover
{
    background-color: #d5e1ed;
}

QWidget#search_widget QPushButton::pressed
{
    background-color: #b1bbc4;
}

QWidget#search_widget QLineEdit
{
    font-size: 14px;
    color: #000000;
    background-color: #ffffff;
    border: 1px solid darkgray;
    border-radius: 5px;
}

