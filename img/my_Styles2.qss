QCheckBox::indicator
{
    background-color: #ffffff;
    border: 1px solid darkgray;
    border-radius: 5px;
    width: 20px;
    height: 20px;
}

QCheckBox::indicator:checked
{
    image:url(":/img/checkbox_isChecked1.png");
    width: 20px;
    height: 20px;
    background-color: #ffffff;
    border: 1px solid darkgray;
}

QCheckBox::indicator:unchecked:hover
{
    background-color: #dbf3ff;
}

